<h1>Vehicles#index</h1>
<p>Find me in app/views/vehicles/index.html.erb</p>
   <div>   
        <%= link_to 'Add Vehicle', new_vehicle_path %>   
      </div>   

<table class="table table-dark">   
      <thead>   
      <tr>   
        <th scope="col">Name</th>   
        <th scope="col">Price</th>   
        <th scope="col">picture</th>   
        <th scope="col">created</th>   
          <th scope="col">updated</th>   
             <th scope="col">Actions</th>   
      </tr>   
      </thead>   
      <tbody>   
      <% @vehicles.each do |vehicle| %>   
          <tr>   
            <td><%= vehicle.name %></td>   
            <td><%= vehicle.price %></td>   
          
            <td><img class="mycar" src=<%=vehicle.picture%> /></td>   
            <td><%= vehicle.created_at %></td>   
            <td><%= vehicle.updated_at %></td>   
     <td>   
               <div>   
                <%= link_to 'Show', vehicle_path(vehicle) %>   
                <%= link_to 'Edit', edit_vehicle_path(vehicle) %>   
                <%= link_to 'Delete',vehicle_path(vehicle), method: :delete %>   
              </div>   
            </td>   

          </tr>   
      <% end %>   
      </tbody>   
    </table>   
