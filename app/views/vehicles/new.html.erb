<h1>Vehicles#new</h1>
 <div class="container">   
  <%= form_for @vehicle, url: {action: :create} do |f| %>   
      <div>   
        <h3>Add a Vehicle</h3>   
      </div>   
      <div>   
        <p>   
          <%= "<div>#{@vehicle.errors.full_messages.first}</div>".html_safe if @vehicle.errors.any? %>   
        <div>   
            <label>Vehicle Name</label>   
              <%= f.text_field :name %>   
        </div>   
  
        <div>   
             <label>Price</label>   
             <%= f.text_field :price %>   
        </div>   
  
        <div>   
           <label>Image URL</label>   
              <%= f.text_field :picture %>   
        </div>   
         </p>   
      </div>   
      <div>   
        <%= link_to 'Back', { controller: 'vehicles', action: 'index'} %>   
        <%= f.submit 'Create Vehicle' %>   
      </div>   
  <% end %>   
</div>  